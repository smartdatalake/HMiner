{
    "indir": "../../data/gdelt/nodes/",
    "irdir": "../../data/gdelt/relations/",
    "algorithm": "DynP",
    "hin_out": "./HIN.csv",
    "analysis_out": "./ANALYSIS.csv",
    "analysis_out_type": "verbose",
    "query": {
        "metapath": "APA",
        "src_field": "name",
        "dest_field": "name",
        "buildIndex": "false",
        "constraints": {
            "A": "id < 100"
        }
    }, 
    "ranking": {
        "ranking_out": "./RANKING.csv",
        "pr_alpha": 0.5,
        "pr_tol": 0.00000000001,
        "threshold": 2
    }
}
